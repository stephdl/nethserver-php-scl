max_execution_time                     = {
    my $et = $php54{MaxExecutionTime} || "30";
    $OUT .= "$et";
}
memory_limit                           = {
    my $ml = $php54{MemoryLimit} || "125";
    $OUT .= "$ml"."M";
}
;added by stephdl see $php54{MaxInputTime}
max_input_time                     = {
    my $it = $php54{MaxInputTime} || "60";
    $OUT .= "$it";
}

