max_execution_time                     = {
    my $et = $php71{MaxExecutionTime} || "30";
    $OUT .= "$et";
}
memory_limit                           = {
    my $ml = $php71{MemoryLimit} || "125";
    $OUT .= "$ml"."M";
}
;added by stephdl see $php71{MaxInputTime}
max_input_time                     = {
    my $it = $php71{MaxInputTime} || "60";
    $OUT .= "$it";
}
