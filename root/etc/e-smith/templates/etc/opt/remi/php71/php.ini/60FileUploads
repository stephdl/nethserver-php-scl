file_uploads                           = On
upload_max_filesize                    = {
    my $mf = $php71{UploadMaxFilesize} || "10";
    $OUT .= "$mf"."M";
}
; added by stephdl see $php71{MaxFileUpload}
max_file_uploads                       = {
    my $mfu = $php71{MaxFileUpload} || "20";
    $OUT .= "$mfu";
}
